{
  "name": "levlang",
  "displayName": "LevLang",
  "description": "Syntax highlighting for LevLang game development language",
  "version": "0.1.0",
  "publisher": "levelium",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "levlang",
    "game development",
    "pygame",
    "transpiler"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sriramramnath/language"
  },
  "contributes": {
    "languages": [
      {
        "id": "levlang",
        "aliases": [
          "LevLang",
          "levlang"
        ],
        "extensions": [
          ".lvl"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "levlang",
        "scopeName": "source.levlang",
        "path": "./syntaxes/levlang.tmLanguage.json"
      }
    ]
  }
}
